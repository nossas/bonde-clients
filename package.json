{
  "name": "bonde-client",
  "version": "0.7.0",
  "description": "Bonde Client - Node.js Server and React + Redux Progressive Web App",
  "author": "Nossas <tech@nosssas.org>",
  "homepage": "http://bonde.org",
  "scripts": {
    "test": "./node_modules/.bin/standard && node --max_old_space_size=4096 ./node_modules/.bin/jest",
    "test:ci": "yarn test --ci --silent --runInBand --logHeapUsage",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "nyc yarn test",
    "start": "webpack-dev-server --config ./tools/webpack.dev.js",
    "build:client": "./node_modules/.bin/webpack --colors --hide-modules --bail -p --config ./tools/webpack.prod.js",
    "build:webviewer": "./node_modules/.bin/webpack --colors --hide-modules --bail --config ./tools/webpack.webviewer.js",
    "clean": "rm -rf build",
    "routes": "grep -rhe @route ./routes/ | sort",
    "routes-old": "grep -rhe @path ./routes/ | sort",
    "release": "./node_modules/.bin/standard-version",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c ./client/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nossas/bonde-client.git"
  },
  "keywords": [],
  "license": "GPL-3.0",
  "dependencies": {
    "assets-webpack-plugin": "^3.5",
