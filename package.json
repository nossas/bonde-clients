{
  "name": "bonde-client",
  "version": "0.5.11",
  "description": "Bonde Client - Node.js Server and React + Redux Progressive Web App",
  "author": "Nossas <tech@nosssas.org>",
  "homepage": "http://bonde.org",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "nyc yarn test",
    "start": "nodemon -r 'babel-register' --delay 1 --watch ./server --watch ./tools ./server",
    "start:prod": "node ./build/server.js",
    "build:server": "./node_modules/.bin/webpack --colors --hide-modules --bail -p --config ./tools/webpack.server.js",
    "build:client": "./node_modules/.bin/webpack --colors --hide-modules --bail --config ./tools/webpack.client.js",
    "clean": "rm -rf build",
    "routes": "grep -rhe @path ./routes/ | sort",
    "release": "./node_modules/.bin/standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nossas/bonde-client.git"
  },
  "keywords": [],
  "license": "GPL-3.0",
  "dependencies": {
