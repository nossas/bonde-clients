  "standard": {
    "globals": [
      "it",
      "describe",
      "expect",
      "afterAll",
      "beforeAll",
      "beforeEach",
      "before",
      "after",
      "afterEach",
      "Blob",
      "wysihtml5",
      "wysihtml5ParserRules",
      "PagarMeCheckout",
      "confirm"
    ],
    "parser": "babel-eslint"
  },
  "engines": {
    "engineStrict": true,
    "node": "~8"
  },
  "postcss": {
    "map": true,
    "plugins": {
      "autoprefixer": {}
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/migrations/**",
      "**/tools/**",
      "**/test/**"
    ]
  },
  "jest": {
    "roots": [
      "test",
      "client",
      "routes"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "wysihtml-toolbar.min.js$": "<rootDir>/tools/jest/mock-file.js",
      "^~(.*)$": "<rootDir>$1",
      "^~client(.*)$": "<rootDir>/client$1",
      "^~routes(.*)$": "<rootDir>/routes$1",
      "^~server(.*)$": "<rootDir>/server$1",
      "^~tools(.*)$": "<rootDir>/tools$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|wav|mp3|aac)$": "<rootDir>/tools/jest/mock-file.js",
      "\\.(css|scss)$": "<rootDir>/tools/jest/mock-style.js"
    }
  }
}
