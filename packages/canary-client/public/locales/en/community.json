{
  "info": {
    "form": {
      "fields": {
        "image": {
          "label": "Community logo"
        },
        "name": {
          "label": "Name",
          "placeholder": "Enter your community name",
          "errors": {
            "required": "Enter community name"
          }
        },
        "description": {
          "label": "Description",
          "placeholder": "Enter a description for your community"
        },
        "city": {
          "label": "City",
          "placeholder": "Host city or focus of activity",
          "errors": {
            "required": "Tell us which city your community operates in"
          }
        },
        "email_template_from": {
          "label": "Standard email",
          "placeholder": "Ex: Name of sender <sender@provider.com>",
          "hint": "This email is used as the default sender of notifications.",
          "errors": {
            "format": "Response email out of standard format"
          }
        }
      }
    }
  },
  "integrations": {
    "form": {
      "fields": {
        "mailchimp_api_key": {
          "label": "Mailchimp API Key",
          "placeholder": "Enter here the content of \"API key\"",
          "help": {
            "title": "Where to get this information?",
            "step1": "After logging in as administrator in mailchimp, click on your username. A menu will appear, click the {accountStrong} option.",
            "step2": "Follow the steps: {extrasStrong}> {apiKeysStrong}> {yourApiKeysStrong}> {createKeyStrong}",
            "step3": "Now just paste the contents of {apiKeyStrong} into the field below."
          }
        },
        "mailchimp_list_id": {
          "label": "Mailchimp List ID",
          "placeholder": "Enter here the \"List ID\"",
          "help": {
            "title": "Where to get this information?",
            "step1": "After logging in as administrator in mailchimp, click on your username. A menu will appear, click the {listStrong} option.",
            "step2": "Select the corresponding list and follow the steps: {settingsStrong}> {listAndDefaultsStrong}",
            "step3": "Now just paste in the field below the contents of the right column below the title {listIdStrong}"
          }
        },
        "twilio_account_sid": {
          "label": "Twilio Account SID",
          "help": {
            "title": "Where to get this information?",
            "step1": "Sign in to {link} Twilio's website"
          }
        },
        "twilio_auth_token": {
          "label": "Twilio Auth Token",
          "help": {
            "title": "Where to get this information?",
            "step1": "Click the eye icon"
          }
        },
        "twilio_number": {
          "label": "Twilio Number",
          "help": {
            "title": "Where to get this information?",
            "step1": "Click {strongPhoneNumber} ({strongHashtag} symbol in the left menu)",
            "step2": "Choose which number Twilio wants to use"
          }
        }
      },
      "buttons": {
        "syncronize": "Synchronize"
      }
    }
  },
  "recipient": {
    "form": {
      "titles": {
        "transfer_date": "Scheduling of Looting",
        "account": "Bank account"
      },
      "fields": {
        "transfer_interval": {
          "label": "Recurrence",
          "options": {
            "weekly": "Weekly",
            "monthly": "Monthly"
          }
        },
        "transfer_day": {
          "label": "Execution day",
          "options": {
            "mon": "Monday",
            "tue": "Tuesday",
            "wed": "Fourth",
            "thu": "Fifth",
            "fri": "Friday"
          }
        },
        "bank_account": {
          "type": {
            "label": "Account Type",
            "options": {
              "conta_corrente": "Chain",
              "conta_poupanca": "Savings"
            }
          },
          "bank_code": {
            "label": "Bank",
            "options": {
              "empty": "Select the bank"
            },
            "errors": {
              "required": "Required field"
            }
          },
          "agencia": {
            "label": "Agency",
            "placeholder": "Enter numbers only",
            "errors": {
              "required": "Required field"
            }
          },
          "agencia_dv": {
            "label": "Digit",
            "errors": {
              "required": "Required field"
            }
          },
          "conta": {
            "label": "Account",
            "placeholder": "Enter numbers only",
            "errors": {
              "required": "Required field"
            }
          },
          "conta_dv": {
            "label": "Digit",
            "errors": {
              "required": "Required field"
            }
          },
          "legal_name": {
            "label": "Name / Company Name",
            "placeholder": "Ex: My Sampa",
            "errors": {
              "required": "Required field"
            }
          },
          "document_number": {
            "label": "CPF / CNPJ",
            "placeholder": "Enter numbers only",
            "errors": {
              "required": "Required field",
              "cpf": "Invalid CPF",
              "cnpj": "Invalid CNPJ"
            }
          }
        }
      }
    }
  },
